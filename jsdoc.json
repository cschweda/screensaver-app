{
  "source": {
    "include": ["src", "scripts"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark"
    }
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  }
}
